{
    "modules": {
        "storage.connections": {
            "class": "\\Reactor\\KVStorage\\Module",
            "config": {
                "dynamic": {
                    "connections": {
                        "local-memcache": {
                            "type": "memcache",
                            "servers": [{
                                "host": "localhost",
                                "port": 11211
                            }]
                        },
                        "local-redis": {
                            "type": "redis",
                            "servers": [{
                                "scheme": "tcp",
                                "host": "localhost",
                                "port": 6379
                            }]
                        }
                    }
                }
            }
        }
    },
    "dynamic": {
        "cache": "@storage.connections/local-memcache"
    }
}