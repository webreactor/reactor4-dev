{
    "modules": {
        "storage.services": {
            "class" :"\\Reactor\\KVStorage\\Module",
            "config": {
                "dynamic": {
                    "services" : {
                        "memcache": {
                            "connections": {
                                "local-memcache": {
                                    "type": "memcache",
                                    "server1": {
                                        "host": "localhost",
                                        "port": 11211
                                    }
                                }
                            }
                        },
                        "redis": {
                            "connections": {
                                "local-redis": {
                                    "type": "redis",
                                    "server1": {
                                        "scheme" : "tcp",
                                        "host": "localhost",
                                        "port": 6379
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "dynamic": {
        "cache": "@storage.services/local-memcache"
    }
}
