{
    "modules": {
        "wrapper.services": {
            "class" :"\\Reactor\\Wrapper\\Module",
            "config": {
                "dynamic": {
                    "services" : {
                        "memcache": {
                            "class": "\\Reactor\\Wrapper\\Memcache",
                            "connections": {
                                "local-memcache": {
                                    "server1": {
                                        "host": "localhost",
                                        "port": 11211
                                    }
                                }
                            }
                        },
                        "redis": {
                            "class": "\\Reactor\\Wrapper\\Redis",
                            "connections": {
                                "local-redis": {
                                    "server1": {
                                        "scheme" : "tcp",
                                        "host": "localhost",
                                        "port": 6379
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "dynamic": {
        "cache": "@wrapper.services/local-memcache"
    }
}
