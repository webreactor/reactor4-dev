{
    "modules": {
        "events": {
            "class": "\\Reactor\\Events\\Module\\SubscribersLoaderHelper",
            "config": {
                "dynamic": {
                    "listeners": {
                        "web-app.received": { "websession": "onLoad" }
                    }
                }
            }
        }
    },
    "services": {
        "handler": {
            "shared": true,
            "create": "\\Reactor\\WebSession\\Handler",
            "arguments": [
                "@storage",
                "@max_life_time"
            ]
        }
    },
    "parameters": {
        "max_life_time": 1800
    }
}
